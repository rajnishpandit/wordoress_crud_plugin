<script>
	jQuery( document ).ready(function() {
var targetDiv = jQuery('body');

jQuery(window).scroll(function() {
	alert('tttt');

     var windowpos = jQuery(window).scrollTop();

     if( windowpos >= 50 ) {
          targetDiv.addClass('scrolling-active');
     } else {
          targetDiv.removeClass('scrolling-active');
     }

});
	});
</script>
